<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#009688"
    tools:context=".ResultActivity">


    <TextView
        android:id="@+id/username"
        android:layout_width="77dp"
        android:layout_height="21dp"
        android:gravity="center"
        android:text="Name"
        android:layout_marginTop="140dp"
        android:layout_marginStart="50dp"
        android:textColor="#FFFFFF"
        />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="431dp"
        android:layout_height="680dp"
        app:srcCompat="@drawable/page_bg"
        tools:layout_editor_absoluteY="285dp"
        android:layout_marginStart="-130dp"
        android:layout_marginTop="170dp"/>

    <ImageView
        android:id="@+id/imageCal"
        android:layout_width="137dp"
        android:layout_height="76dp"
        android:layout_marginStart="-200dp"
        android:layout_marginTop="40dp"
        app:srcCompat="@drawable/default_btn"
        tools:layout_editor_absoluteX="251dp"
        tools:layout_editor_absoluteY="36dp" />

    <ImageView
        android:id="@+id/imageProfile"
        android:layout_width="136dp"
        android:layout_height="136dp"
        android:layout_marginStart="-340dp"
        android:layout_marginTop="15dp"
        app:srcCompat="@drawable/profile"
         />

    <Button
        android:id="@+id/result_btn_logout"
        android:layout_width="139dp"
        android:layout_height="37dp"
        android:text="Logout"
        tools:layout_editor_absoluteX="271dp"
        tools:layout_editor_absoluteY="681dp"
        android:layout_marginTop="690dp"
        android:layout_marginStart="70dp"/>

    <TextView
        android:id="@+id/text_view_date_1"
        android:layout_width="148dp"
        android:layout_height="28dp"
        android:gravity="center"
        android:text="time"
        android:textSize="18dp"
        android:layout_marginTop="650dp"
        android:layout_marginStart="-370dp"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="645dp" />

    <Button
        android:id="@+id/button_date_1"
        android:layout_width="143dp"
        android:layout_height="34dp"
        android:text="Change Date"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="681dp"
        android:layout_marginTop="690dp"
        android:layout_marginStart="-128dp"/>

    <TextView
        android:id="@+id/ShowCal"
        android:layout_width="84dp"
        android:layout_height="27dp"
        android:text="ShowCal"
        android:textSize="20sp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="60dp"
        tools:layout_editor_absoluteX="271dp"
        tools:layout_editor_absoluteY="55dp" />



    <TextView
        android:id="@+id/textView5"
        android:layout_width="200dp"
        android:layout_height="500dp"
        android:layout_marginStart="-337dp"
        android:layout_marginTop="170dp"
        android:background="@color/colorAccent"
        android:text="  " />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="200dp"
        android:layout_height="500dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="170dp"
        android:background="#66CC33"
        android:text="  " />


    <ImageButton
        android:id="@+id/AddFood"
        android:layout_width="13dp"
        android:layout_height="20dp"
        android:layout_marginStart="-300dp"
        android:layout_marginTop="640dp"
        app:srcCompat="@drawable/edit" />
    <ImageButton
        android:id="@+id/AddEx"
        android:layout_width="13dp"
        android:layout_height="20dp"
        android:layout_marginStart="190dp"
        android:layout_marginTop="640dp"
        app:srcCompat="@drawable/edit" />

    <TextView
        android:id="@+id/foodlist1"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:gravity="center"
        android:layout_marginStart="-320dp"
        android:layout_marginTop="180dp"
        android:text="food list" />

    <Button
        android:id="@+id/editprofile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Button" />

</LinearLayout>